{
  "name": "scanit-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=5000 nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"!src/database/migrations/**\"",
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=5000 nest start",
    "start:server": "cross-env NODE_ENV=development nest start --watch",
    "start:local": "cross-env NODE_ENV=development npm run db:migrate && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development npm run db:migrate && cross-env NODE_ENV=development nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=development npm run db:migrate && cross-env NODE_ENV=development && NODE_OPTIONS=--max_old_space_size=5000 nest start",
    "start:qa": "cross-env NODE_ENV=qa npm run db:migrate && cross-env NODE_ENV=qa && NODE_OPTIONS=--max_old_space_size=5000 nest start",
    "start:test": "cross-env NODE_ENV=test npm run db:migrate && cross-env NODE_ENV=test node dist/main",
    "start:prod": "cross-env NODE_ENV=production npm run db:migrate && cross-env NODE_ENV=production node dist/main",
    "lint": "eslint --ignore-pattern \"src/database/migrations/**\" --fix \"{src,apps,libs,test}/**/*.ts\"",
    "test": "cross-env NODE_ENV=test npm run db:migrate && jest --coverage",
    "test:local": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test npm run db:migrate && jest --watch",
    "test:cov": "cross-env NODE_ENV=test npm run db:migrate && jest --clear-cache && jest --detectOpenHandles --coverage --max_old_space_size=5000",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/database/ormconfig.ts",
    "typeorm-without-dataSource": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "create:migration": "npm run typeorm-without-dataSource -- migration:create",
    "db:migrate": "npm run typeorm -- migration:run",
    "generate:migration": "npm run typeorm -- migration:generate",
    "db:revert": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@aws-sdk/url-parser": "^3.374.0",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "amazon-cognito-identity-js": "^6.3.12",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "graphql-scalars": "^1.23.0",
    "jwks-rsa": "^3.1.0",
    "moment": "^2.30.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-transactional": "^0.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
